#include <iostream>
using namespace std;

int main()
{
    int arr1[5], i ;
int *ptr ;

cout << "\n Enter the numbers:-" ;
for( i = 0; i < 5; i++ )
{
cin >> arr1[i];
}
ptr = arr1 ;
for( i = 0; i < 5; i++)
{
cout << *ptr ;
ptr++ ;
}
}
