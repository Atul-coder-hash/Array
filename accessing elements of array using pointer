#include<iostream>
using namespace std;


int main()
{
   int a[5],i,*ptr;
   int sum(int[],int);
   for(i=0;i<5;i++)
   {cout<<"enter number:-";
       cin>>a[i];
   }
   for(i=0;i<5;i++)
   {
       ptr=&a[i];
       cout<<*ptr;
   }
   
}

