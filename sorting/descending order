// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
   int a[]={1,4,3,2,5},i,j,n=5;
   for(i=0;i<n-1;i++)
   {
       for(j=0;j<n-i-1;j++)
       {
           int temp;
           if(a[j]<a[j+1])
           {
               temp=a[j];
               a[j]=a[j+1];
               a[j+1]=temp;
           }
       }
   }
   
    for(i=0;i<n;i++)
    {
        cout<<a[i];
    }
    
}
