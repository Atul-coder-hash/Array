#include<iostream>
using namespace std;


void printarray(int a[2][3])
{
    for(int i=0;i<2;i++)
    {
        for(int j=0;j<3;j++)
        {
            cout<<a[i][j]<<"\t";
        }
        cout<<endl;
    }
}
void enteringarray(int a[2][3])
{
    for(int i=0;i<2;i++)
    {
        for(int j=0;j<3;j++)
        {cout<<"enter element:-";
            cin>>a[i][j];
        }
        cout<<endl;
    }
    
    printarray(a);
}
int main()
{int r,c;
    
    int a[2][3];
    enteringarray(a);
}
