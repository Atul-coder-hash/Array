#include<iostream>
using namespace std;                


int main()
{
int i,j,a[2][3];

for(i=0;i<=1;i++)
{
    for(j=0;j<=2;j++)
    {
        cout<<"enter:-";
        cin>>a[i][j];
    }
}


for(i=0;i<=1;i++)
{
    for(j=0;j<=2;j++)
    {
        
        cout<<a[i][j];
        
    }
    cout<<"\n";
}


}
