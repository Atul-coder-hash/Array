
#include <iostream>
using namespace std;
int main() {
    int a[]={1,5,3,8,7},n=5,i,j;
    for(int i=0;i<n-i;i++)
    {
        for(int j=0;j<n-i+1;i++)
        {
            if(a[j]>a[j+1])
            {
                int temp;
                temp=a[j];
                a[j]=a[j+1];
                a[j+1]=temp;
                
            }
        }
    }
    for(i=0;i<n-i;i++)
    {
        cout<<a[j];
    }
}
